{
  "name": "mi-proyecto-react",
  "private": true,
  "version": "0.0.0",
  "author": "Lucia Lozano",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder --win",
    "preview": "vite preview",
    "test": "jest --coverage"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-jest": "^30.2.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "build": {
    "appId": "com.lucialozano.pruebaelectron",
    "productName": "Prueba Electron",
    "win": {
      "target": [
      {
        "target": "nsis",
        "arch": ["x64"]
      }
    ],
      "icon": "public/OlalaHomes.ico"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true, 
      "allowToChangeInstallationDirectory": false,
      "installerIcon": "public/OlalaHomes.ico",
      "uninstallerIcon": "public/OlalaHomes.ico",
      "createDesktopShortcut": true,
      "runAfterFinish": false
    },
    "directories": {
      "output": "dist-executable"
    },
    "forceCodeSigning": false
  }
}
